                        p.pros-percent 80%
                        p.pros-text   Понад 12000 моделей (автомобілі, мотоцикли та інтер'єри)
                    li.pros-item.element-animation
                        .pros-image-wrap 
                            img.pros-image(src="img/icons/ic3.svg", alt="shield")
                        p.pros-percent 120
